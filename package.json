{
  "name": "fetch-engine",
  "description": "A smart request-making library that makes sure your Javascript client is a good citizen of your distributed system.",
  "main": "dist/fetch-engine-node.js",
  "browser": "dist/fetch-engine-browser.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && tsc",
    "watch": "npm run clean && tsc --watch",
    "test": "npm run specs && npm run lint",
    "lint": "find src -name '*.ts' | xargs tslint",
    "specs-node": "tape $(find ./dist -name '*.test.js' | grep -v browser) | tap-min",
    "specs-zuul": "npm run build && zuul -- dist/test-browser.js",
    "specs": "npm run build && npm run specs-node && npm run specs-zuul",
    "zuul": "zuul --no-coverage --local 9998 -- dist/test-browser.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "files": [
    "dist"
  ],
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phuu/fetch-engine.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/phuu/fetch-engine/issues"
  },
  "homepage": "https://github.com/phuu/fetch-engine#readme",
  "devDependencies": {
    "boom": "^4.2.0",
    "browserify": "^13.0.0",
    "es6-promise": "^4.0.5",
    "ghooks": "^1.2.0",
    "hapi": "^15.1.1",
    "npm-check": "^5.2.1",
    "require-globify": "^1.3.0",
    "semantic-release": "^4.3.5",
    "tap-min": "^1.1.0",
    "tape": "^4.6.3",
    "tape-run": "^2.1.5",
    "tslint": "^3.7.1",
    "typescript": "^2.1.5",
    "typings": "^1.4.0",
    "yargs": "^6.6.0",
    "zuul": "^3.10.1"
  },
  "dependencies": {
    "parse-headers": "^2.0.1"
  }
}
